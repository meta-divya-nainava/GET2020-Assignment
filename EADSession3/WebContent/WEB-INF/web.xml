  </servlet>
  <servlet-mapping>
    <servlet-name>updatedetail</servlet-name>
    <url-pattern>/updateDetails</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>updatetable</servlet-name>
    <servlet-class>com.details.UpdateTable</servlet-class>
