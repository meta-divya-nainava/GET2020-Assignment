  <servlet-mapping>
    <servlet-name>adddetail</servlet-name>
    <url-pattern>/AddStudent</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>searchdetail</servlet-name>
    <servlet-class>com.details.SearchStudentDetail</servlet-class>
  </servlet>
